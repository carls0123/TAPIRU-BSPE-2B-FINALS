 import numpy as np

#letters JRE [10,18,5]
#letters AAS [1,1,19]

#matrix1
matrix1 = np.array([[10,18,5],[1,1,19]] )
print("MATRIX 1:\n", matrix1)

#first 3 SN digits [2,0,2]
#last 3 SN digits [4,8,2]

#matrix2
matrix2 = np.array([[2,0,2],[4,8,2]])
print("\nMATRIX 2:\n", matrix2)

