7/21:
 import numpy as np

#letters JRE
list1 = [10,18,5]
#letters AAS
list2 = [1,1,19]

#matrix1
matrix1 = [list1,list2]
print("MATRIX 1:\n", matrix1)

#first 3 SN digits
list3 = [2,0,2]
#last 3 SN digits
list4 = [4,8,2]

#matrix2
matrix2 = [list3,list4]
print("MATRIX 2:\n", matrix2)

#matrix addition
matrix3 = np.add(matrix1,matrix2)
print("MATRIX 3:\n", matrix3)

#scalar multiplication
matrix4 = np.multiply(matrix1,2)
print("MATRIX 4:\n", matrix4)
